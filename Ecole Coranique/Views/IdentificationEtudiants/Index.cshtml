@model IEnumerable<Ecole_Coranique.Models.IdentificationEtudiant>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Etudiant)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdentityUser)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Etudiant.Fullname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdentityUser.UserName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EtudiantId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EtudiantId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EtudiantId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
