@model IEnumerable<Ecole_Coranique.Models.Revision>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Etudiant)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Huitieme)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Hizb)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Etudiant.Fullname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Huitieme.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Hizb.Nom)
            </td>
        </tr>
}
    </tbody>
</table>
